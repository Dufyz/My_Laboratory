comando_mysql = f'SELECT *from clientes'
        cursor.execute(comando_mysql)
        dbcliente = cursor.fetchall()

        print('\n CLIENTE REGISTRADOS: ')

        for c in dbcliente:
            print(f'\nNome: {c[0]}')
            print(f'Sobrenome: {c[1]}')
            print(f'Idade: {c[2]}')
            print(f'Compras: {c[3]}')


